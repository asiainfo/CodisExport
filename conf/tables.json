{
  "footfall-Callerloc": {
    "header":"area_code,province_id",
    "dateFormat":"yyyyMMdd",
    "timeFormat":"yyyyMMddHHmm",
    "conditions":{
      "area_code":{
        "like":"111|888",
        "sign":"&&"
      }
    },
    "where":"area_code||province_id"
  },
  "footfall-residence": {
    "header":"area_code,pspt_eparchy_id",
    "dateFormat":"yyyy-MM-dd",
    "timeFormat":"yyyy-MM-dd HH:mm",
    "conditions":{
      "area_code":{
        ">":800,
        "sign":"||"
      },
      "pspt_eparchy_id":{
        "eq":"abc",
        "sign":"&&"
      }
    },
    "where":"area_code||pspt_eparchy_id"
  }
}



